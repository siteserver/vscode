{
	"name": "vscode-sscms",
	"displayName": "SSCMS",
	"description": "SSCMS STL Language",
	"version": "1.1.0",
	"icon": "siteserver.png",
	"publisher": "sscms",
	"license": "MIT",
	"engines": {
		"vscode": "^1.38.0"
	},
	"repository": {
		"url": "https://github.com/siteserver/vscode"
	},
	"keywords": [
		"CMS",
		"SSCMS",
		"SiteServer"
	],
	"categories": [
		"Extension Packs"
	],
	"author": "SSCMS",
	"bugs": {
	  "url": "https://github.com/siteserver/vscode/issues"
	},
	"homepage": "https://github.com/siteserver/vscode#readme",
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"colors": [
			{
				"id": "stl.background",
				"description": "Background decoration color for large numbers",
				"defaults": {
					"dark": "#ff000055",
					"light": "#ff000055",
					"highContrast": "#ff000055"
				}
			}
		],
		"html": {
			"customData": [
				"./stl/stl_a.json",
				"./stl/stl_action.json",
				"./stl/stl_audio.json",
				"./stl/stl_channel.json",
				"./stl/stl_channels.json",
				"./stl/stl_container.json",
				"./stl/stl_content.json",
				"./stl/stl_contents.json",
				"./stl/stl_count.json",
				"./stl/stl_dynamic.json",
				"./stl/stl_each.json",
				"./stl/stl_file.json",
				"./stl/stl_focusViewer.json",
				"./stl/stl_if.json",
				"./stl/stl_image.json",
				"./stl/stl_include.json",
				"./stl/stl_itemTemplate.json",
				"./stl/stl_layer.json",
				"./stl/stl_loading.json",
				"./stl/stl_location.json",
				"./stl/stl_map.json",
				"./stl/stl_marquee.json",
				"./stl/stl_material.json",
				"./stl/stl_more.json",
				"./stl/stl_navigation.json",
				"./stl/stl_no.json",
				"./stl/stl_pageChannels.json",
				"./stl/stl_pageContents.json",
				"./stl/stl_pageItem.json",
				"./stl/stl_pageItems.json",
				"./stl/stl_pageSqlContents.json",
				"./stl/stl_pdf.json",
				"./stl/stl_player.json",
				"./stl/stl_printer.json",
				"./stl/stl_query.json",
				"./stl/stl_queryString.json",
				"./stl/stl_request.json",
				"./stl/stl_search.json",
				"./stl/stl_select.json",
				"./stl/stl_share.json",
				"./stl/stl_site.json",
				"./stl/stl_sites.json",
				"./stl/stl_sqlContent.json",
				"./stl/stl_sqlContents.json",
				"./stl/stl_swiper.json",
				"./stl/stl_tabs.json",
				"./stl/stl_tags.json",
				"./stl/stl_user.json",
				"./stl/stl_value.json",
				"./stl/stl_video.json",
				"./stl/stl_yes.json",
				"./stl/stl_zoom.json"
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "tslint -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/node": "^8.10.25",
		"tslint": "^5.16.0",
		"typescript": "^3.5.1",
		"@types/vscode": "^1.38.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"source-map-support": "^0.5.12",
		"vscode-test": "^1.3.0"
	}
}
